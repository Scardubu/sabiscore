[run]
branch = True
source = 
    backend/src
omit = 
    backend/src/api/websocket.py
    backend/src/cli/*
    backend/src/connectors/*
    backend/src/models/*
    backend/src/data/scrapers.py
    backend/src/core/redis.py

[report]
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.
    raise NotImplementedError
    except ImportError
    except Exception as e
